{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "GameGateway": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:7000"
      }
    },
    "Limits": {
      "MaxRequestBodySize": 10485760,
      "RequestHeadersTimeout": "00:00:30"
    }
  },
  "GameGateway": {
    "Codec": {
      "Mode": "PassThrough",
      "Compression": {
        "Enabled": false,
        "Type": "Deflate"
      },
      "Encryption": {
        "Enabled": false,
        "Algorithm": "AES256GCM",
        "Key": null,
        "Nonce": null
      }
    },
    "Session": {
      "CookieName": "uuid",
      "CookiePath": "/",
      "UseRedis": false,
      "RedisConnectionString": "localhost:6379"
    },
    "Game": {
      "CurrentVersion": 365957,
      "TTSCdnUri": "https://ba.dn.nexoncdn.co.kr/tts/version2/",
      "ServerTimezoneOffset": 0
    },
    "LoadBalancer": {
      "Cookies": {
        "LB": "node1",
        "LBCORS": "cors1"
      }
    },
    "Testing": {
      "DeterministicMode": false,
      "FixedSeed": 12345
    },
    "Security": {
      "StrictTransportSecurity": {
        "Enabled": true,
        "MaxAge": 2592000
      },
      "HeaderCasing": {
        "LowerCase": false
      }
    }
  }
}